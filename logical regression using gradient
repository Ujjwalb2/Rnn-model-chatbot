        "def modify_dataframe(dataset):\n",
        "  dataset=dataset.drop(columns=['PassengerId','Name'])\n",
        "  dataset['Ticket']= label_encoder_1.fit_transform(dataset['Ticket'])\n",
        "  dataset['Cabin']= label_encoder_2.fit_transform(dataset['Cabin'])\n",
        "  # dataset['HomePlanet']= label_encoder_3.fit_transform(dataset['HomePlanet'])\n",
        "  dataset['Sex'] = dataset['Sex'].map({'male': 1, 'female': 0})\n",
        "  dataset['Embarked'] = dataset['Embarked'].map({'S': 1, 'C': 0})\n",
        "  dataset=dataset.fillna(dataset.mean(numeric_only=True))\n",
        "  dataset = dataset.astype(float)\n",
        "  return dataset\n"
      ],
      "metadata": {
        "id": "TvD5a6MJXulD"
