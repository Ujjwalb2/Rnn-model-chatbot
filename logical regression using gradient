        "# converting h\n",
        "def h_to_bollean(h):\n",
        "  n=h.shape[0]\n",
        "  for i in range(n):\n",
        "    if(h[i]>=0.5):\n",
        "      h[i]=1\n",
        "    else:\n",
        "      h[i]=0\n",
        "  return h"
      ],
      "metadata": {
