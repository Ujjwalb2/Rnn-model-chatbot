      "cell_type": "code",
      "source": [
        "def sigmoid(x):\n",
        "    return 1/(1 + np.exp(-x))"
      ],
      "metadata": {
        "id": "ANkyjGY7cn1S"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def model(x,y,lr,num_iterations,lamba):\n",
        "  n,d=x.shape\n",
        "  w=np.reshape(np.random.rand(d),(-1,1))\n",
        "  for i in range(num_iterations):\n",
        "    y_cap=np.dot(X,w)\n",
        "    h=sigmoid(y_cap)\n",
        "    gradient=(1/n)*(np.dot(X.T,h-y)+lamda*w)\n",
        "    w=w-lr*gradient\n",
        "  return w,h\n"
      ],
      "metadata": {
        "id": "isovwlO7dies"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "lr=0.01\n",
        "num_iterations=80000\n",
        "lamda=0.2"
      ],
      "metadata": {
        "id": "JrKzQ-3_iN8O"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "w,h=model(x,y,lr,num_iterations,lamda)\n"
      ],
      "metadata": {
